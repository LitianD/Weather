<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimaryDark">#3A5FCD</item>
        <item name="colorPrimary">#3A5FCD</item>
        <item name="colorAccent">#3A5FCD</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
    </style>

    <style name="PopUpToolbar" parent="Theme.AppCompat.Light">
        <item name="android:background">#3A5FCD</item>
    </style>

    <style name="LaunchTheme" parent="AppTheme">
        <item name="android:windowBackground">@drawable/lunch</item>
    </style>

    <style name="common_mProgress_circle">
        <item name="android:indeterminateDuration">900</item>
        <item name="android:indeterminateDrawable">@drawable/progress_cirle</item>
        <item name="android:minWidth">30dp</item>
        <item name="android:minHeight">30dp</item>
        <item name="android:maxWidth">60dp</item>
        <item name="android:maxHeight">60dp</item>
    </style>
</resources>
